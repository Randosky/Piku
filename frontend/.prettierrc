{
  "trailingComma": "es5",
  "singleQuote": false,
  "tabWidth": 2,
  "semi": true,
  "printWidth": 120,
  "useTabs": false,
  "jsxSingleQuote": false,
  "arrowParens": "avoid",
  "singleAttributePerLine": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/layout/(.*)$",
    "^@/ui/(.*)$",
    "^@/providers/(.*)$",
    "^@/constants/(.*)$",
    "^@/schemes/(.*)$",
    "^@/shared/(.*)$",
    "^@/assets/(.*)$",
    "^@/config/(.*)$",
    "^@/store/(.*)$",
    "^@/redux/(.*)$",
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "^@/api/(.*)$",
    "(.view)$",
    "(.container)$",
    "(.service)$",
    "(.config)$",
    "(.types)$",
    "(.scss)$",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-organize-imports", "@trivago/prettier-plugin-sort-imports"]
}
